kind: ConfigMap
apiVersion: v1
metadata:
  name: mq-configmap
data:
  mq.ini: |
    SSL:
    OutboundSNI=HOSTNAME
  mq.mqsc: |   
  ALTER QMGR CONNAUTH('') CHLAUTH(DISABLED)
  DEF QL('NHSHL7.ADT') MAXDEPTH(100000) DEFPSIST(YES) REPLACE
  DEF QL('NHSHL7.ORU_R01') MAXDEPTH(100000) DEFPSIST(YES) REPLACE
  DEF QL('NHSHL7.RXF1') MAXDEPTH(100000) DEFPSIST(YES) REPLACE
  DEF QL('NHSHL7.DATASTAGE') MAXDEPTH(100000) DEFPSIST(YES) REPLACE
  DEF QL('NHSHL7.SRC') MAXDEPTH(100000) DEFPSIST(YES) REPLACE
  DEF QL('NHSHL7.DEST1') MAXDEPTH(100000) DEFPSIST(YES) REPLACE
  DEF QL('NHSHL7.ERR') MAXDEPTH(100000) DEFPSIST(YES) REPLACE
  DEF QL('NHSHL7.SEQNOS') DEFPSIST(YES) REPLACE
  DEF QL('NHSHL7.SEQNTFY') DEFPSIST(YES) REPLACE 
